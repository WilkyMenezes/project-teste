<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="containerDois">  
        <% alimentos.forEach( item2 => { %> 
        <div class="itemDois">
            <a href="/alimentos/<%= item2.id %>">
                <h2><%= item2.nome %></h2>
                <p><%= item2.descricao %></p>
                <img src="<%= item2.imagem %>" alt="capa do filme">
            </a></div>    
        <% }); %>
    </div>
</body>
</html>