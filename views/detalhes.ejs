<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="container">  
        <% comidas.forEach( item => { %> 
        <div class="item">
            <a href="/comidas/<%= item.id %>">
                <h2><%= item.nome %></h2>
                <p><%= item.descricao %></p>
                <img src="<%= item.imagem %>" alt="capa do filme">
            </a></div>   
        <% }); %>
    </div>
</body>
</html>